{
  "name": "rehearsal-scheduler",
  "version": "1.0.0",
  "description": "A web application for scheduling band rehearsals, tracking attendance, and suggesting optimal rehearsal times.",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxaginfo/rehearsal-scheduler-2025-06-18.git"
  },
  "keywords": [
    "band",
    "rehearsal",
    "scheduler",
    "music",
    "availability"
  ],
  "author": "DX AG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dxaginfo/rehearsal-scheduler-2025-06-18/issues"
  },
  "homepage": "https://github.com/dxaginfo/rehearsal-scheduler-2025-06-18#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.10",
    "sequelize": "^6.35.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}